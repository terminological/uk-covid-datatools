@article{luAreCollegeCampuses2020,
  title = {Are College Campuses Superspreaders? {{A}} Data-Driven Modeling Study},
  shorttitle = {Are College Campuses Superspreaders?},
  author = {Lu, Hannah and Weintz, Cortney and Pace, Joseph and Indana, Dhiraj and Linka, Kevin and Kuhl, Ellen},
