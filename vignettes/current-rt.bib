@article{rodriguesAssociationUseMeningococcus2020,
  title = {Association of {{Use}} of a {{Meningococcus Group B Vaccine With Group B Invasive Meningococcal Disease Among Children}} in {{Portugal}}},
  author = {Rodrigues, Fernanda M. P. and Marlow, Robin and Simões, Maria João and Danon, Leon and Ladhani, Shamez and Finn, Adam},
  date = {2020-12-01},
  journaltitle = {JAMA},
