@article{fontanaUnderstandingViralSheddingundefined/ed,
  title = {Understanding {{Viral Shedding}} of {{SARS-CoV-2}}: {{Review}} of {{Current Literature}}},
  shorttitle = {Understanding {{Viral Shedding}} of {{SARS-CoV-2}}},
  author = {Fontana, Lauren and Villamagna, Angela Holly and Sikka, Monica K. and McGregor, Jessina C.},
  year = {undefined/ed},
  journaltitle = {Infection Control \& Hospital Epidemiology},
