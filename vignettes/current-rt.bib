@article{kuanChronologicalMap3082019,
  title = {A Chronological Map of 308 Physical and Mental Health Conditions from 4 Million Individuals in the {{English National Health Service}}},
  author = {Kuan, Valerie and Denaxas, Spiros and Gonzalez-Izquierdo, Arturo and Direk, Kenan and Bhatti, Osman and Husain, Shanaz and Sutaria, Shailen and Hingorani, Melanie and Nitsch, Dorothea and Parisinos, Constantinos A and Lumbers, R Thomas and Mathur, Rohini and Sofat, Reecha and Casas, Juan P and Wong, Ian C K and Hemingway, Harry and Hingorani, Aroon D},
  date = {2019-06},
  journaltitle = {Lancet Digit Health},
  volume = {1},
