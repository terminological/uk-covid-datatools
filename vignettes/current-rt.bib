  keywords = {electronic patient records,PRIMARY CARE,scientometrics,STATISTICS & RESEARCH METHODS},
  file = {/home/terminological/Zotero/storage/HXVULX6Z/Vezyridis and Timmons - 2016 - Evolution of primary care databases in UK a scien.pdf;/home/terminological/Zotero/storage/5L4GNU46/e012785.html}
