  langid = {english},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Editorial Subject\_term: Diagnosis;Outcomes research;Publication characteristics Subject\_term\_id: diagnosis;outcomes-research;publication-characteristics},
  file = {/home/terminological/Zotero/storage/C2G9XL44/Kakarmath et al. - 2020 - Best practices for authors of healthcare-related a.pdf;/home/terminological/Zotero/storage/XL8N8RU8/s41746-020-00336-w.html}
