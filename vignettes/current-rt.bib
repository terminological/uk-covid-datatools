@article{caduffInferringTransmissionFitness2021,
  title = {Inferring {{Transmission Fitness Advantage}} of {{SARS-CoV-2 Variants}} of {{Concern}} in {{Wastewater Using Digital PCR}}},
  author = {Caduff, Lea and Dreifuss, David and Schindler, Tobias and Devaux, Alexander J. and Ganesanandamoorthy, Pravin and Kull, Anina and Stachler, Elyse and Fernandez-Cassi, Xavier and Beerenwinkel, Niko and Kohn, Tamar and Ort, Christoph and Julian, Timothy R.},
