  keywords = {big data,depression,Facebook,screening,social media},
  file = {/home/terminological/Zotero/storage/86SSSSXD/Eichstaedt et al. 2018 - Facebook language predicts depression in medical records.pdf;/home/terminological/Zotero/storage/HWJYGFWM/Eichstaedt et al. - 2018 - Facebook language predicts depression in medical r.pdf}
