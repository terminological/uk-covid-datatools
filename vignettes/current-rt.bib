@article{whalenTimeSeriesDisturbance2018,
  title = {Time Series Disturbance Detection for {{Hypothesis-Free}} Signal Detection in Longitudinal Observational Databases},
  author = {Whalen, Ed and Hauben, Manfred and Bate, Andrew},
  date = {2018-06},
