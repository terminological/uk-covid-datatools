@article{brandForecastingScaleCOVID192020,
  title = {Forecasting the Scale of the {{COVID-19}} Epidemic in {{Kenya}}},
  author = {Brand, Samuel P. C. and Aziza, Rabia and Kombe, Ivy K. and Agoti, Charles N. and Hilton, Joe and Rock, Kat S. and Parisi, Andrea and Nokes, D. James and Keeling, Matt and Barasa, Edwine},
