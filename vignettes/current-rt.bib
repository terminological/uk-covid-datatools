  author = {Lescure, Francois-Xavier and Bouadma, Lila and Nguyen, Duc and Parisey, Marion and Wicky, Paul-Henri and Behillil, Sylvie and Gaymard, Alexandre and Bouscambert-Duchamp, Maude and Donati, Flora and Hingrat, Quentin Le and Enouf, Vincent and Houhou-Fidouh, Nadhira and Valette, Martine and Mailles, Alexandra and Lucet, Jean-Christophe and Mentre, France and Duval, Xavier and Descamps, Diane and Malvy, Denis and Timsit, Jean-François and Lina, Bruno and van-der- Werf, Sylvie and Yazdanpanah, Yazdan},
  options = {useprefix=true},
  date = {2020-06-01},
  journaltitle = {The Lancet Infectious Diseases},
