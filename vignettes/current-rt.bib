@article{falsterUsingWeightedHospital2018,
  title = {Using {{Weighted Hospital Service Area Networks}} to {{Explore Variation}} in {{Preventable Hospitalization}}},
  author = {Falster, Michael O. and Jorm, Louisa R. and Leyland, Alastair H.},
