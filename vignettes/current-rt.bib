  author = {Tindale, Lauren C and Stockdale, Jessica E and Coombe, Michelle and Garlock, Emma S and Lau, Wing Yin Venus and Saraswat, Manu and Zhang, Louxin and Chen, Dongxuan and Wallinga, Jacco and Colijn, Caroline},
