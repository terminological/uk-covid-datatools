@article{hsiehCharacteristicsConsequencesDrug2004,
  title = {Characteristics and Consequences of Drug Allergy Alert Overrides in a Computerized Physician Order Entry System},
  author = {Hsieh, Tyken C and Kuperman, Gilad J and Jaggi, Tonushree and Hojnowski-Diaz, Patricia and Fiskio, Julie and Williams, Deborah H and Bates, David W and Gandhi, Tejal K},
  date = {2004},
  journaltitle = {Journal of the American Medical Informatics Association},
  shortjournal = {J. Am. Med. Inform. Assoc.},
  volume = {11},
  number = {6},
  pages = {482--491},
  abstract = {The aim of this study was to determine characteristics of drug allergy alert overrides, assess how often they lead to preventable adverse drug events (ADEs), and suggest methods for improving the allergy-alerting system. Chart review was performed on a stratified random subset of all allergy alerts occurring during a 3-month period (August through October 2002) at a large academic hospital. Factors that were measured were drug/allergy combinations that triggered alerts, frequency of specific override reasons, characteristics of ADEs, and completeness of allergy documentation. A total of 6,182 (80\%) of 7,761 alerts were overridden in 1,150 patients. In this sample, only 10\% of alerts were triggered by an exact match between the drug ordered and allergy listed. Physicians' most common reasons for overriding alerts were “Aware/Will monitor” (55\%), “Patient does not have this allergy/tolerates” (33\%), and “Patient taking already” (10\%). In a stratified random subset of 320 patients (28\% of 1,150) on chart review, 19 (6\%) experienced ADEs attributed to the overridden drug; of these, 9 (47\%) were serious. None of the ADEs was considered preventable, because the overrides were deemed clinically justifiable. The degree of completeness of patients' allergy lists was highly variable and generally low in both paper charts and the CPOE system. Overrides of drug-allergy alerts were common and about 1 in 20 resulted in ADEs, but all of the overrides resulting in ADEs appeared clinically justifiable. The high rate of alert overrides was attributable to frequent nonexact match alerts and infrequent updating of allergy lists. Based on these findings, we have made specific recommendations for increasing the specificity of alerting and thereby improving the clinical utility of the drug allergy alerting system.},
  file = {/home/terminological/Zotero/storage/C63AU8MT/Hsieh et al. - 2004 - Characteristics and consequences of drug allergy a.pdf}
}

