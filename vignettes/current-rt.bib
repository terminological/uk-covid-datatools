@article{hongIntegratingStructuredUnstructured2018,
  title = {Integrating Structured and Unstructured {{EHR}} Data Using an {{FHIR-based}} Type System: {{A}} Case Study with Medication Data},
  author = {Hong, Na and Wen, Andrew and Shen, Feichen and Sohn, Sunghwan and Liu, Sijia and Liu, Hongfang and Jiang, Guoqian},
