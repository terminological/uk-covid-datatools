@article{kwokEpidemiologicalCharacteristicsFirst2020,
  title = {Epidemiological Characteristics of the First 53 Laboratory-Confirmed Cases of {{COVID-19}} Epidemic in {{Hong Kong}}, 13 {{February}} 2020},
  author = {Kwok, Kin On and Wong, Valerie Wing Yu and Wei, Wan In and Wong, Samuel Yeung Shan and Tang, Julian Wei-Tze},
