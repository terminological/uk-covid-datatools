@article{meredithRapidImplementationRealtime2020,
  title = {Rapid Implementation of Real-Time {{SARS}}-{{CoV}}-2 Sequencing to Investigate Healthcare-Associated {{COVID}}-19 Infections},
  author = {Meredith, Luke W. and Hamilton, William L. and Warne, Ben and Houldcroft, Charlotte J. and Hosmillo, Myra and Jahun, Aminu and Curran, Martin D. and Parmar, Surendra and Caller, Laura and Caddy, Sarah L. and Khokhar, Fahad A. and Yakovleva, Anna and Hall, Grant R. and Feltwell, Theresa and Forret, Sally N. and Sridhar, Sushmita and p Weekes, Michael and Baker, Stephen and Brown, Nicholas and Moore, Elinor and Gouliouris, Theodore and Popay, Ashley and Roddick, Iain and Reacher, Mark and Peacock, Sharon and Dougan, Gordon and Torok, M. Estee and Goodfellow, Ian},
  date = {2020-05-14},
  journaltitle = {medRxiv},
  pages = {2020.05.08.20095687},
  publisher = {{Cold Spring Harbor Laboratory Press}},
  doi = {10.1101/2020.05.08.20095687},
  abstract = {{$<$}p{$>$}Background The burden and impact of healthcare-associated COVID-19 infections is unknown. We aimed to examine the utility of rapid sequencing of SARS-CoV-2 combined with detailed epidemiological analysis to investigate healthcare-associated COVID-19 infections and to inform infection control measures. Methods We set up rapid viral sequencing of SARS-CoV-2 from PCR-positive diagnostic samples using nanopore sequencing, enabling sample-to-sequence in less than 24 hours. We established a rapid review and reporting system with integration of genomic and epidemiological data to investigate suspected cases of healthcare-associated COVID-19. Results Between 13 March and 24 April 2020 we collected clinical data and samples from 5191 COVID-19 patients in the East of England. We sequenced 1000 samples, producing 747 complete viral genomes. We conducted combined epidemiological and genomic analysis of 299 patients at our hospital and identified 26 genomic clusters involving 114 patients. 66 cases (57.9\%) had a strong epidemiological link and 15 cases (13.2\%) had a plausible epidemiological link. These results were fed back clinical, infection control and hospital management teams, resulting in infection control interventions and informing patient safety reporting. Conclusions We established real-time genomic surveillance of SARS-CoV-2 in a UK hospital and demonstrated the benefit of combined genomic and epidemiological analysis for the investigation of healthcare-associated COVID-19 infections. This approach enabled us to detect cryptic transmission events and identify opportunities to target infection control interventions to reduce further healthcare-associated infections.{$<$}/p{$>$}},
  file = {/home/terminological/Zotero/storage/VFQ7MSD5/Meredith et al. - 2020 - Rapid implementation of real-time SARS-CoV-2 seque.pdf;/home/terminological/Zotero/storage/TH37HR46/2020.05.08.html},
  langid = {english}
}

