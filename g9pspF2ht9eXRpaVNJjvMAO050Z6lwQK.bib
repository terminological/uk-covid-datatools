@article{trimbleMortalityAssociatedAlcoholrelated,
  title = {Mortality Associated with Alcohol-Related Liver Disease},
  author = {Trimble, G. and Zheng, L. and Mishra, A. and Kalwaney, S. and Mir, H. M. and Younossi, Z. M.},
