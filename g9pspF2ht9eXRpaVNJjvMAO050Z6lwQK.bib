@article{herrettValidationValidityDiagnoses2010,
  title = {Validation and Validity of Diagnoses in the {{General Practice Research Database}}: A Systematic Review},
  shorttitle = {Validation and Validity of Diagnoses in the {{General Practice Research Database}}},
  author = {Herrett, Emily and Thomas, Sara L. and Schoonen, W. Marieke and Smeeth, Liam and Hall, Andrew J.},
  date = {2010},
  journaltitle = {British Journal of Clinical Pharmacology},
  volume = {69},
