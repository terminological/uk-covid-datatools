@article{narkiewiczRelationFamilyHistory1995,
  title = {Relation between Family History of Hypertension, Overweight and Ambulatory Blood Pressure: The {{HARVEST}} Study},
  author = {Narkiewicz, K. and Gatti, P. and Garavelli, G. and Pegoraro, F. and Milani, L. and Gelisio, R. and Palatini, P.},
