  file = {/home/terminological/Zotero/storage/ZNWGSVNN/WANNAMETHEE2007.pdf},
  keywords = {Abdominal Fat,Adiposity,Adult,Aged,Body Composition,Body Mass Index,Humans,Male,Middle Aged,Mortality,Muscle; Skeletal,Waist-Hip Ratio},
  number = {5}
}

