  shorttitle = {Effect of {{Vascular Risk Factors}} and {{Diseases}} on {{Mortality}} in {{Individuals}} with {{Dementia}}},
  author = {van de Vorst, Irene E. and Koek, Huiberdina L. and de Vries, Rehana and Bots, Michiel L. and Reitsma, Johannes B. and Vaartjes, Ilonca},
  date = {2016-01-01},
  journaltitle = {Journal of the American Geriatrics Society},
  shortjournal = {J Am Geriatr Soc},
