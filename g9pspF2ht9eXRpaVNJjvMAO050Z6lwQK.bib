@article{mykletunLevelsAnxietyDepression2009,
  title = {Levels of Anxiety and Depression as Predictors of Mortality: The {{HUNT}} Study},
  shorttitle = {Levels of Anxiety and Depression as Predictors of Mortality},
  author = {Mykletun, Arnstein and Bjerkeset, Ottar and Øverland, Simon and Prince, Martin and Dewey, Michael and Stewart, Robert},
