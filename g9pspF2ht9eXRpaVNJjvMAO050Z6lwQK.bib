@article{gaoImpactCOPDEmphysema2016,
  title = {Impact of {{COPD}} and Emphysema on Survival of Patients with Lung Cancer: {{A}} Meta-Analysis of Observational Studies},
  shorttitle = {Impact of {{COPD}} and Emphysema on Survival of Patients with Lung Cancer},
  author = {Gao, Yong-hua and Guan, Wei-jie and Liu, Qi and Wang, Hua-qi and Zhu, Ya-nan and Chen, Rong-chang and Zhang, Guo-jun},
