  author = {Stortecky, Stefan and Schoenenberger, Andreas W. and Moser, André and Kalesan, Bindu and Jüni, Peter and Carrel, Thierry and Bischoff, Seraina and Schoenenberger, Christa-Maria and Stuck, Andreas E. and Windecker, Stephan and Wenaweser, Peter},
