@article{gottfridssonSexDifferenceFactors2008,
  title = {Sex Difference and Factors Associated with Outcome in Patients with Sustained Ventricular Arrhythmias},
  author = {Gottfridsson, Christer and Nyström, Britta and Karlsson, Thomas and Herlitz, Johan and Edvardsson, Nils},
