  file = {/home/terminological/Zotero/storage/9JEVKS3G/HEYNESFIELD2011.pdf},
  keywords = {Adipose Tissue,Adult,African Americans,Body Composition,Body Fluid Compartments,Body Height,Body Mass Index,Body Weight,European Continental Ancestry Group,Female,Humans,Male,Mexican Americans,Middle Aged,Nutrition Surveys,Obesity,Reference Values,Regression Analysis,Sex Factors},
