@article{duBronchiectasisComorbidityChronic2016,
  title = {Bronchiectasis as a {{Comorbidity}} of {{Chronic Obstructive Pulmonary Disease}}: {{A Systematic Review}} and {{Meta}}-{{Analysis}}},
  shorttitle = {Bronchiectasis as a {{Comorbidity}} of {{Chronic Obstructive Pulmonary Disease}}},
