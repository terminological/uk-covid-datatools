@article{hjerlDepressionPrognosticFactor2003,
  title = {Depression as a {{Prognostic Factor}} for {{Breast Cancer Mortality}}},
  author = {Hjerl, Karen and Andersen, Elisabeth W. and Keiding, Niels and Mouridsen, Henning T. and Mortensen, Preben B. and Jørgensen, Torben},
