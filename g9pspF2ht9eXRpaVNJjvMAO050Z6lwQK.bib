  keywords = {Adaptation; Physiological,Aged,Aged; 80 and over,Aging,Antioxidants,Biological Evolution,Clinical Trials as Topic,Dietary Supplements,Hormone Replacement Therapy,Hypertension,Metabolic Syndrome X},
  number = {2}
