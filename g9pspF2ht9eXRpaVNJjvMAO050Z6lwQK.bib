  author = {Andersson, Tobias and Hjerpe, Per and Carlsson, Axel C. and Pivodic, Aldina and Wändell, Per and Manhem, Karin and Bengtsson Boström, Kristina},
  date = {2018-04},
  journaltitle = {Diabetes Research and Clinical Practice},
