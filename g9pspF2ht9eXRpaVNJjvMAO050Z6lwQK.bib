@article{wangEffectsAlcoholDrinking2017,
  title = {Effects of Alcohol Drinking and Smoking on Pancreatic Ductal Adenocarcinoma Mortality: {{A}} Retrospective Cohort Study Consisting of 1783 Patients},
  shorttitle = {Effects of Alcohol Drinking and Smoking on Pancreatic Ductal Adenocarcinoma Mortality},
  author = {Wang, Chengfeng and Zhao, Dongbing and Chen, Haibo and Huang, Huang and Zhang, Jianwei and Ma, Jie and Jiang, Qinglong and Zhang, Shuisheng and Yuan, Wei and Che, Xu and Zhao, Yajie and Tian, Yantao and Zhang, Yawei and Chen, Yingtai and Sun, Yuemin and Chu, Yunmian},
