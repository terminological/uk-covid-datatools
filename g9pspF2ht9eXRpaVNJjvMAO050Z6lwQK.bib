  eprinttype = {pmid},
  file = {/home/terminological/Zotero/storage/H3P6A93U/Gitsels et al. - 2017 - Survival prospects after acute myocardial infarcti.pdf;/home/terminological/Zotero/storage/J7DNUG9U/bmjopen-2016-013570supp.pdf},
  number = {1},
  pmcid = {PMC5278246}
}

