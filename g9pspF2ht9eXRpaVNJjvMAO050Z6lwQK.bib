  number = {2}
}

@article{hewittRespiratorySymptomsOlder2005,
  title = {Respiratory Symptoms in Older People and Their Association with Mortality},
  author = {Hewitt, J. and Smeeth, L. and Bulpitt, C. J. and Tulloch, A. J. and Fletcher, A. E.},
  date = {2005-04-01},
  journaltitle = {Thorax},
  volume = {60},
