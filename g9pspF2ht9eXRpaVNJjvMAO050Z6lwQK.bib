@article{celenPrognosticImpactPositive2007,
  title = {Prognostic Impact of Positive Lymph Node Ratio in Gastric Carcinoma},
  author = {Celen, Orhan and Yildirim, Emin and Berberoglu, Uğur},
  date = {2007-08-01},
  journaltitle = {Journal of Surgical Oncology},
