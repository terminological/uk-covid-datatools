  eprinttype = {pmid},
  keywords = {acetylcholine receptor antibody,Adolescent,Adult,Aged,Aged; 80 and over,Antibodies,Community Health Planning,Denmark,Female,Follow-Up Studies,Humans,Kaplan-Meier Estimate,Male,Middle Aged,mortality rate ratio,myasthenia gravis,Myasthenia Gravis,Proportional Hazards Models,Receptors; Cholinergic,Retrospective Studies,seropositive myasthenia gravis,Young Adult},
