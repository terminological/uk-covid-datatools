@article{gatheralCOPDrelatedBronchiectasisIndependent2014,
  title = {{{COPD}}-Related {{Bronchiectasis}}; {{Independent Impact}} on {{Disease Course}} and {{Outcomes}}},
  author = {Gatheral, Timothy and Kumar, Neelam and Sansom, Ben and Lai, Dilys and Nair, Arjun and Vlahos, Ioannis and Baker, Emma H.},
