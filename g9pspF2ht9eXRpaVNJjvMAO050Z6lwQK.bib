  author = {Galsky, Matthew D. and Moshier, Erin and Krege, Susan and Lin, Chia-Chi and Hahn, Noah and Ecke, Thorsten and Sonpavde, Guru and Godbold, James and Oh, William K. and Bamias, Aristotle},
  date = {2013-08-15},
  journaltitle = {Cancer},
  shortjournal = {Cancer},
  volume = {119},
