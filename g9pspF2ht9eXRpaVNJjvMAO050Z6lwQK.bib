  eprinttype = {pmid},
  keywords = {Adult,Carbonated Beverages,Cost Control,Cost of Illness,Diabetes Mellitus,Female,Fructose,Health Expenditures,Health Surveys,Humans,Male,Middle Aged,Obesity,Taxes},
  number = {1}
}

@article{wangRestingEnergyExpenditure2001,
