  author = {Herrett, Emily and Bhaskaran, Krishnan and Timmis, Adam and Denaxas, Spiros and Hemingway, Harry and Smeeth, Liam},
  date = {2014-09-14},
  journaltitle = {European Heart Journal},
  shortjournal = {Eur Heart J},
  volume = {35},
