  number = {1}
}

@article{herrmannEffectsAnxietyDepression2000,
  title = {Effects of Anxiety and Depression on 5-Year Mortality in 5057 Patients Referred for Exercise Testing},
  author = {Herrmann, Christoph and Brand-Driehorst, Sigrid and Buss, Ullrich and Rüger, Ulrich},
  date = {2000-04},
  journaltitle = {Journal of Psychosomatic Research},
