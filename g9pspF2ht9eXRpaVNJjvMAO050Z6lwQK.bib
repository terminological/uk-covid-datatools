@article{mcgeeBodyMassIndex2005,
  title = {Body Mass Index and Mortality: A Meta-Analysis Based on Person-Level Data from Twenty-Six Observational Studies},
  author = {McGee, D.L.},
  date = {2005-02},
  journaltitle = {Ann.Epidemiol.},
  shortjournal = {Ann.Epidemiol.},
